<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" crossorigin="anonymous">
<%- include('../partials/header') %>
<div class="container mt-4">
    <h3>Tambah Kelas</h3>
    <form method="POST" action="/admin/kelas/tambah">
        <input type="text" name="nama_kelas" class="form-control mb-2" placeholder="Nama Kelas" required>
        <textarea name="deskripsi" class="form-control mb-2" placeholder="Deskripsi Kelas"></textarea>
        <input type="text" name="gambar" class="form-control mb-2" placeholder="Nama Gambar">
        <button class="btn btn-success">Simpan</button>
    </form>
</div>
<%- include('../partials/footer') %>